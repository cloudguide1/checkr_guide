    }, function() {
      var aria_controls = $(this).attr("aria-controls");
        //alert(aria_controls);
        $("#main-toc #"+aria_controls).removeClass("expanded").addClass("collapse");
    }); */
    
    
    
    $("#main-toc .sidebar-category").click(function() {
      var aria_controls = $(this).attr("aria-controls");
      //alert(aria_controls);
      $("#main-toc #"+aria_controls).removeClass("collapse").addClass("expanded");
    });
    
  });
</script>




