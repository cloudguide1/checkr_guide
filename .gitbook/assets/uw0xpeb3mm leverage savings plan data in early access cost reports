<script type="text/javascript">
    (function() {
        $(function() {
          $('#article img').each(function() {
            $(this).wrap('<a></a>');
            $(this).parent().addClass('lightbox').attr('href', $(this).attr('src'));
          });
          $('.lightbox').magnificPopup({
              type: 'image',
              closeOnContentClick: true,
              mainClass: 'mfp-img-mobile',
              image: {
                  verticalFit: true
              }
          });
        });
    })();
</script>
