                  <script>
                    function toggleCategoryIcon (event, category_id) {
                      var $p = $('#category-' + category_id + '-title');
                      if ($p.hasClass('expanded')) {
                        $p.removeClass('expanded');
                      } else {
                        $p.addClass('expanded');
                      }
                    }
